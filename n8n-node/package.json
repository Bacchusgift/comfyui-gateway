{
  "name": "n8n-nodes-comfyui-gateway",
  "version": "0.1.0",
  "description": "n8n node for ComfyUI Gateway: submit workflows, get task status and history",
  "license": "MIT",
  "keywords": ["n8n-community-node-package", "comfyui", "gateway"],
  "author": { "name": "Bacchusgift" },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bacchusgift/comfyui-gateway.git",
    "directory": "n8n-node"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": ["dist"],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": ["dist/credentials/ComfyUiGatewayApi.credentials.js"],
    "nodes": ["dist/nodes/ComfyUiGateway/ComfyUiGateway.node.js"]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "typescript": "~5.9.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
